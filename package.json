{
	"name": "themeswitcher",
	"displayName": "ThemeSwitcher",
	"description": "Swaps theme based on time of day",
	"publisher": "MohnishKalia",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Themes"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "ThemeSwitcher",
			"properties": {
				"themeswitcher.startDay": {
					"type": "number",
					"default": 7,
					"description": "The hour from 0-23 you want light theme to start."
				},
				"themeswitcher.endDay": {
					"type": "number",
					"default": 15,
					"description": "The hour from 0-23 you want dark theme to start."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/MohnishKalia/ThemeSwitcher"
	}
}
